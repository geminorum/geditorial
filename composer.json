{
  "name": "geminorum/geditorial",
  "description": "Our Editorial in WordPress",
  "license": "GPL-3+",
  "type": "wordpress-plugin",
  "keywords": [
    "editorial",
    "meta"
  ],
  "homepage": "https://geminorum.ir/wordpress/geditorial",
  "authors": [{
    "name": "Nasser Rafie",
    "email": "contact@geminorum.ir",
    "homepage": "https://geminorum.ir",
    "role": "Developer"
  }],
  "support": {
    "issues": "https://github.com/geminorum/geditorial/issues",
    "wiki": "https://github.com/geminorum/geditorial/wiki",
    "source": "https://github.com/geminorum/geditorial"
  },
  "require": {
    "php": ">=5.4",
    "composer/installers": "^1.5",
    "mustache/mustache": "^2.12",
    "kzykhys/php-csv-parser": "^1.4",
    "nicmart/string-template": "^0.1.1",
    "michelf/php-markdown": "^1.8",
    "league/html-to-markdown": "^4.6"
  },
  "require-dev": {
    "nesbot/carbon": "^1.27",
    "evert/sitemap-php": "^1.2",
    "eluceo/ical": "^0.11.4",
    "johngrogg/ics-parser": "^2.1",
    "sabre/vobject": "^4.1",
    "farhadi/intl-datetime": "dev-master",
    "lawondyss/moment-php": "dev-master",
    "amirhabibzadeh/intl-carbon": "dev-master",
    "addwiki/mediawiki-api": "^0.5.1",
    "seboettg/citeproc-php": "^2"
  },
  "config": {
    "vendor-dir": "assets/vendor",
    "vendor/bin": "assets/bin",
    "platform": {
      "php": "5.6.35"
    }
  },
  "autoload": {
    "psr-4": {
      "geminorum\\gEditorial\\": "includes/"
    }
  }
}
