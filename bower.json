{
  "name": "geditorial",
  "private": true,
  "dependencies": {
    "jquery-colorbox": "latest",
    "jquery-sortable": "latest",
    "jcarousel": "latest",
    "lity": "latest",
    "virastar": "brothersincode/virastar",
    "persiantools": "Bersam/persiantools",
    "mediaelement-plugins": "mediaelement/mediaelement-plugins",
    "octicons": "primer/octicons",
    "gridicons": "Automattic/gridicons",
    "genericons-neue": "Automattic/genericons-neue",
    "social-logos": "Automattic/social-logos",
    "themicons": "cedaro/themicons",
    "gorbeh": "IKAcc/Gorbeh",
    "list.js": "latest",
    "handsontable": "^7.0.2",
    "medium-editor": "^5.23.3"
  },
  "devDependencies": {},
  "install": {
    "path": "assets/packages",
    "ignore": [
      "jquery",
      "octicons",
      "gridicons",
      "genericons-neue",
      "social-logos",
      "themicons",
      "gorbeh"
    ],
    "sources": {
      "virastar": [
        "assets/components/virastar/lib/virastar.js",
        "assets/components/virastar/lib/virastar.min.js"
      ],
      "jquery-colorbox": [
        "assets/components/jquery-colorbox/jquery.colorbox.js",
        "assets/components/jquery-colorbox/jquery.colorbox-min.js",
        "assets/components/jquery-colorbox/example3/colorbox.css",
        "assets/components/jquery-colorbox/example3/images/controls.png",
        "assets/components/jquery-colorbox/example3/images/loading.gif"
      ],
      "jquery-sortable": [
        "assets/components/jquery-sortable/source/js/jquery-sortable.js",
        "assets/components/jquery-sortable/source/js/jquery-sortable-min.js"
      ],
      "list.js": [
        "assets/components/list.js/dist/list.js",
        "assets/components/list.js/dist/list.min.js"
      ],
      "persiantools": [
        "assets/components/persiantools/dist/persiantools.js",
        "assets/components/persiantools/dist/persiantools.min.js"
      ],
      "mediaelement-plugins": {
        "mapping": [{
          "assets/components/mediaelement-plugins/dist/source-chooser/**": "source-chooser"
        }]
      }
    }
  }
}
