<div class="-wrap -image-summaries">
	{{#covers}}{{#url}}
		<div class="image_card">
			<div class="image_card-photo_content -{{key}}-image {{class}}">
				<a class="image_card-link" href="{{{url}}}" target="_blank">
					<img src="{{{url}}}" class="image_card-photo img-fluid" alt="">
					<div class="image_card-hint">{{{i18n.viewfullimage}}}</div>
				</a>
			</div>
			<div class="image_card-content">
				<p class="image_card-name"><code>{{{filename}}}</code></p>
				{{! <p class="image_card-title"></p> }}
			</div>
		</div>

	{{/url}}{{/covers}}
</div>
